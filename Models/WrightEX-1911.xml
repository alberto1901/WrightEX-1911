  <animation> 
    <type>material</type> 
    <object-name>prop_l</object-name>
    <object-name>prop_r</object-name>
    <transparency>
       <alpha-prop>engines/engine[0]/rpm</alpha-prop>
       <factor>-0.003</factor>
       <offset>1</offset>
    </transparency> 
  </animation>

  <animation>
    <type>select</type>
    <object-name>prop_l</object-name>
    <object-name>prop_r</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>320</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <name>Prop_L_Complete</name>
    <object-name>prop_disc_l</object-name>
    <object-name>prop_l</object-name>
  </animation>

  <animation>
    <name>Prop_R_Complete</name>
    <object-name>prop_disc_r</object-name>
    <object-name>prop_r</object-name>
  </animation>

<!--animation> 
  <type>material</type> 
  <object-name>prop_disc_l</object-name>
  <object-name>prop_disc_r</object-name>
    <transparency>
       <alpha-prop>engines/engine[0]/rpm</alpha-prop>
       <factor>-0.0007</factor>
       <offset>1</offset>
    </transparency> 
</animation-->

  <!-- Select objects that become visible with engine rpm -->
  <animation>
    <type>select</type>
    <object-name>prop_disc_l</object-name>
    <object-name>prop_disc_r</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>250</value>
      </greater-than>
    </condition>
  </animation>

  <!-- Prop Spins -->
  <animation>
    <type>spin</type>
    <object-name>Prop_L_Complete</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> 1.617175 </x-m>
      <y-m> -1.682981 </y-m>
      <z-m> 0.000000 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value> 5 </value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>Prop_R_Complete</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> 1.617175 </x-m>
      <y-m> 1.682981 </y-m>
      <z-m> 0.000000 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value> 5 </value>
      </greater-than>
    </condition>
  </animation>


  <!-- Wheel Spins -->
  <animation>
    <type>spin</type>
    <object-name>wheels</object-name>
    <property>gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -0.0321 </x-m>
      <y-m> -0.744151 </y-m>
      <z-m> -1.413938 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Elevator movement, remember there are two halves to represent warp movement -->
  <animation>
    <type>rotate</type>
    <object-name>elevator_aft</object-name>
    <object-name>elevator_fore</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <factor> 20 </factor>
    <center>
      <x-m>  5.345745 </x-m>
      <y-m>  0.001525 </y-m>
      <z-m> -0.080838 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>elevator_aft</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <factor> 20 </factor>
    <center>
      <x-m>  5.709100 </x-m>
      <y-m>  0.001525 </y-m>
      <z-m> -0.080838 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>


  <!-- Rudder Movement -->
  <animation>
    <type>rotate</type>
    <object-name>rudder</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor> 20 </factor>
    <center>
      <x-m>  4.830 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.424 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <!-- Aileron Movement, no ailerons in this wing-warping plane but we can rotate the control sticks instead -->
  <animation>
    <type>rotate</type>
    <object-name>stick_l</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <factor> 6 </factor>
    <center>
      <x-m> -0.408617 </x-m>
      <y-m>  0.448754 </y-m>
      <z-m> -0.704166 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>stick_r</object-name>
    <property>surface-positions/right-aileron-pos-norm</property>
    <factor> 6 </factor>
    <center>
      <x-m> -0.408617 </x-m>
      <y-m> -0.448754 </y-m>
      <z-m> -0.704166 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
